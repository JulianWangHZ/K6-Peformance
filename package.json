{
  "name": "k6-dummyjson-performance-testing",
  "version": "1.0.0",
  "description": "K6 Performance Testing - DummyJSON API",
  "main": "index.js",
  "scripts": {
    "test:load": "k6 run --out json=results/load-test-results.json test_scripts/conduit_load_test.js",
    "test:spike": "k6 run --out json=results/spike-test-results.json test_scripts/conduit_spike_test.js",
    "test:stress": "k6 run --out json=results/stress-test-results.json test_scripts/conduit_stress_test.js",
    "test:all": "npm run test:load && npm run test:spike && npm run test:stress",
    "test:load:dev": "k6 run --out json=results/load-test-dev-results.json test_scripts/conduit_load_test.js",
    "test:load:stage": "k6 run --out json=results/load-test-stage-results.json test_scripts/conduit_load_test.js",
    "test:spike:dev": "k6 run --out json=results/spike-test-dev-results.json test_scripts/conduit_spike_test.js",
    "test:spike:stage": "k6 run --out json=results/spike-test-stage-results.json test_scripts/conduit_spike_test.js",
    "test:stress:dev": "k6 run --out json=results/stress-test-dev-results.json test_scripts/conduit_stress_test.js",
    "test:stress:stage": "k6 run --out json=results/stress-test-stage-results.json test_scripts/conduit_stress_test.js",
    "test:all:dev": "npm run test:load:dev && npm run test:spike:dev && npm run test:stress:dev",
    "test:all:stage": "npm run test:load:stage && npm run test:spike:stage && npm run test:stress:stage",
    "lint": "eslint test_scripts/*.js config/*.js",
    "lint:fix": "eslint test_scripts/*.js config/*.js --fix",
    "validate": "k6 check test_scripts/conduit_load_test.js && k6 check test_scripts/conduit_spike_test.js && k6 check test_scripts/conduit_stress_test.js",
    "clean:results": "rm -rf results/*.json results/*.html",
    "prepare:results": "mkdir -p results"
  },
  "keywords": [
    "k6",
    "performance-testing",
    "load-testing",
    "stress-testing",
    "spike-testing",
    "dummyjson",
    "api-testing",
    "javascript"
  ],
  "author": "Julian Wang",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "k6": {
    "version": ">=0.40.0"
  }
}